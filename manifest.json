{
    "manifest_version": 3,
    "name": "Timer for Focus",
    "version": "1.1",
    "description": "This extension will help you set a timer for your tasks",
    "icons": {
        "16": "icons/clock16.png",
        "48": "icons/clock48.png",
        "128": "icons/clock128.png"
    },
    "action": {
        "default_popup": "app.html"
    },
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "storage",
        "activeTab"
    ]
}